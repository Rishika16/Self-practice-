#Creating a Vector 
V1 <-seq(1,100,by=5) # vector
V1[10]
# [1] 46

V1
# [1]  1  6 11 16 21 26 31 36 41 46 51 56 61 66 71 76 81 86 91 96
length(V1)
[1] 20

#Creating a matrix 
v <-seq(1,100,by=5) # vector
v[10]
# [1] 46

length(v)
# [1] 20

m1<-matrix(v,ncol=5,nrow=10)
m1[,1]
# [1]  1  6 11 16 21 26 31 36 41 46

m1[6,1]
#  [1] 26
